{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npx vite dev",
		"build": "npx vite build",
		"preview": "npx vite preview",
		"prepare": "npx svelte-kit sync || echo ''",
		"check": "npx svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "npx svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint . --cache",
		"test:unit": "vitest",
		"test": "pnpm test:unit -- --run",
		"precommit": "npx tsx ./precommit.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.2",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.2",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^24.10.4",
		"@vitest/browser-playwright": "^4.0.16",
		"daisyui": "^5.5.14",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"prettier-plugin-organize-imports": "^4.3.0",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0",
		"vite": "^7.3.0",
		"vitest": "^4.0.16",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@emotion/css": "^11.13.5",
		"@sveltejs/adapter-node": "^5.4.0",
		"dayjs": "^1.11.19",
		"gridjs": "^6.2.0"
	}
}
